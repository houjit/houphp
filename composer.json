{
    "name": "houjit/houphp",
    "type": "framework",
    "description": "A simple, lightweight and high-performance PHP coroutine framework.",
    "keywords": [
        "php",
        "swoole",
        "houphp",
        "houoole"
    ],
    "license": "Apache-2.0",
    "require": {
        "php": ">=7.4",
        "ext-swoole": ">=4.4",
        "houjit/framework": ">=3.0.0",
        "houjit/db": ">=3.0.0"
    },
    "autoload": {
        "psr-4": {
            "app\\": "app/"
        }
    },
    "bin": [
        "bin/hou.php"
    ],
    "config": {
        "optimize-autoloader": true
    }
}
